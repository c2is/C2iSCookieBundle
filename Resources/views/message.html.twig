<div id="cookiesLegalMessage">
    <div class="inner-content">
        <span class="close">{{ 'c2is.cookie.close'|trans }}</span>
        <p>{{ 'c2is.cookie.accept.message'|trans }}{% if cookieHref|default(false) %} <a href="{{ cookieHref }}" target="_blank">{{ 'c2is.cookie.learn.more'|trans }}</a>{% endif %}   <button type="button" class="cookie-accept">{{ 'c2is.cookie.acknowledge'|trans }}</button></p>
    </div>
</div>
